package test.temp;

import java.util.ArrayList;
import java.util.List;

public class Test1 {

	public static void main(String[] args) {
		
//		List
		

	}
	
	public List<AC> init() {
		List<AC> result = new ArrayList<AC>();
		AC ac1 = new AC(1, "0101,0102");
		AC ac2 = new AC(2, "0201,0202");
		AC ac3 = new AC(3, "0101,0202");
		AC ac4 = new AC(4, "0101,0102,0201,0202");
		AC ac5 = new AC(5, "0101,0102");
		AC ac6 = new AC(6, "0101,0102");
		return result;
	}
	
	class AC {
		private int id;
		private String content;
		public int getId() {
			return id;
		}
		public void setId(int id) {
			this.id = id;
		}
		public String getContent() {
			return content;
		}
		public void setContent(String content) {
			this.content = content;
		}
		public AC(int id, String content) {
			super();
			this.id = id;
			this.content = content;
		}
		
		
	}

}
